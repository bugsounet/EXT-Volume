  ],
  "engines": {
    "node": ">=20"
  },
  "author": "bugsounet",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bugsounet/EXT-Volume/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bugsounet/EXT-Volume"
  },
  "homepage": "https://wiki.bugsounet.fr/en/EXT-Volume",
  "dependencies": {
    "esbuild": "^0.24.0",
    "glob": "^11.0.0"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin": "^2.11.0",
    "eslint": "^9.15.0",
    "eslint-plugin-import": "^2.31.0"
  }
